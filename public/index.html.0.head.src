    <style>
        
        path { 
          stroke: steelblue;
          stroke-width: 2;
          fill: none;
        }
        .axis path,
        .axis line {
          fill: none;
          
          shape-rendering: crispEdges;
        }

        .area {
          fill: steelblue;
        }
        
        .tick text{
            font-size: 12px;
          }

          .tick line{
            opacity: 0.2;
          }
          #container, #containera, #containerb,#Teams_container{width:100%;}
          
          #left,#leftb,#leftt,#leftb_team{float:left;width:300px;height:300px;padding-left:10px;}
          #right,#rightb,#rightt,#rightb_team{float:right;width:300px;height:300px;padding-right:10px;}
          
          .svg-container {
            display: inline-block;
            position: relative;
            width: 100%;
            padding-bottom: 100%; /* aspect ratio */
            vertical-align: top;
            overflow: hidden;
            }
        .svg-content-responsive {
            display: inline-block;
            position: absolute;
            top: 10px;
            left: 0;
        }
    </style>
  </head>

  <body>
